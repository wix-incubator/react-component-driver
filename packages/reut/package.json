{
  "name": "reut",
  "version": "1.0.0",
  "description": "React Test Renderer utils to aid component tests and TDD cycle.",
  "main": "index.js",
  "scripts": {
    "test": "jest -i 2>&1",
    "build": "babel lib --out-dir dist",
    "watch": "chokidar lib -c 'npm run build'",
    "postinstall": "npm run build"
  },
  "author": "Donatas Petrauskas <donatas.petr@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react-test-renderer": "~16.0.0"
  },
  "devDependencies": {
    "babel-cli": "~6.18.0",
    "babel-jest": "^22.0.0",
    "babel-preset-react-native": "^1.9.2",
    "chokidar-cli": "^1.2.0",
    "jest": "^22.0.0",
    "lerna": "^2.11.0",
    "react": "~16.0.0",
    "react-native": "0.51.0",
    "react-redux": "~4.4.6",
    "redux": "~3.6.0",
    "redux-thunk": "~2.1.0"
  },
  "jest": {
    "preset": "react-native",
    "clearMocks": true
  }
}

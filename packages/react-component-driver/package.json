{
  "name": "react-component-driver",
  "version": "0.4.0",
  "description": "React Test Renderer utils to aid component tests and TDD cycle.",
  "main": "index.js",
  "scripts": {
    "test": "jest -i 2>&1",
    "prepare": "rimraf dist && tsc -d"
  },
  "author": "Donatas Petrauskas <donatas.petr@gmail.com>",
  "repository": "github:donataswix/react-component-driver",
  "license": "MIT",
  "peerDependencies": {
    "react-test-renderer": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "^23.6.0",
    "chokidar-cli": "^1.2.0",
    "fast-check": "^1.7.1",
    "jest": "^23.0.0",
    "jest-config": "^23.6.0",
    "metro-react-native-babel-preset": "^0.48.1",
    "react": "^16.5.2",
    "react-native": "^0.57.3",
    "react-test-renderer": "^16.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "react-native",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "clearMocks": true,
    "testRegex": "tests/.*\\.spec\\.(ts|tsx|js)",
    "watchPathIgnorePatterns": [
      "<rootDir>/src/"
    ]
  }
}

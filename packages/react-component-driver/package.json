{
  "name": "react-component-driver",
  "version": "0.3.0",
  "description": "React Test Renderer utils to aid component tests and TDD cycle.",
  "main": "index.js",
  "scripts": {
    "test": "jest -i 2>&1",
    "prepare": "rimraf dist && babel src --out-dir dist",
    "watch": "chokidar lib -c 'npm run prepare'"
  },
  "author": "Donatas Petrauskas <donatas.petr@gmail.com>",
  "repository": "github:donataswix/react-component-driver",
  "license": "MIT",
  "peerDependencies": {
    "react-test-renderer": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-jest": "^23.6.0",
    "chokidar-cli": "^1.2.0",
    "jest": "^23.0.0",
    "metro-react-native-babel-preset": "^0.48.1",
    "react": "^16.5.2",
    "react-native": "^0.57.3",
    "react-test-renderer": "^16.5.0",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "clearMocks": true,
    "testRegex": "tests/.*\\.spec\\.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2"
  }
}
